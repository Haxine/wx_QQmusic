.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

.flexBox {
  display: flex;
  display: -webkit-flex;
}
.flex {
  flex: 1;
}
.flexCenter {
  align-self: center;
}
.nowrap {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}